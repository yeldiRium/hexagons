{
  "extends": "es/node",
  "env": {
    "browser": true
  },
  "rules": {
    "@typescript-eslint/explicit-function-return-type": [2, {
      "allowExpressions": false,
      "allowTypedFunctionExpressions": true,
      "allowHigherOrderFunctions": false
    }],
    "id-length": "off",
    "prefer-destructuring": "off"
  }
}
